{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "scripting",
        "activeTab",
        "notifications"
    ],
    "action": {},
    "icons": {
        "128": "web_slinger_icon.png"
    },
    "background": {
        "service_worker": "scripts/background.js",
        "type": "module"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "game/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}